{
  "name": "node-starter",
  "version": "1.0.0",
  "description": "Node js starter project with webpack, typescript, linting",
  "main": "src/index.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./src/index.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.js --env NODE_ENV=prod",
    "build:prod": "cros-env NODE_ENV=production webpack --config ./webpack/webpack.config.js --env NODE_ENV=prod",
    "build:sit": "cros-env NODE_ENV=sit webpack --config ./webpack/webpack.config.js --env NODE_ENV=sit",
    "lint": "eslint --fix 'src/**/*.{js,json}'",
    "format": "prettier --write './(src|webpack)?/**/*.{js,json}'",
    "test": "jest",
    "test:coverage": "jest --coverage --colors --no-cache --maxWorkers=10",
    "test:watch": "jest --watchAll"
  },
  "keywords": [],
  "author": "mohantalachutla",
  "license": "ISC",
  "homepage": ".",
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "@babel/register": "^7.18.9",
    "babel-jest": "^29.0.1",
    "babel-loader": "^8.2.5",
    "babel-watch": "^7.7.0",
    "corejs": "^1.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^8.0.0",
    "gulp-print": "^5.0.2",
    "husky": "^4.3.8",
    "jest": "^29.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "terser-webpack-plugin": "^5.3.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-dev-server": "^4.10.0",
    "webpack-hot-middleware": "^2.25.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "express": "^4.18.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "formidable": "^2.0.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "uuid": "^8.3.2"
  },
  "lint-staged": {
    "./*/**/*.{js, json}": [
      "eslint --fix"
    ],
    "./(src|webpack)?/**/*.{js,json}": [
      "prettier --write"
    ],
    "./src/**/*.js": [
      "jest --bail --findRelatedTests --passWithNoTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
